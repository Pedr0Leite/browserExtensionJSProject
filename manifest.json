{
    "name":"First App",
    "description":"This is my first app",
    "version":"0.1.0",
    "manifest_version":2,
    "icons":{
        "16": "./surf-wave-icon-16.png"
    },
    "background":{
        "scripts":["thirdParty/jquery-3.6.0.js","./background.js"],
        "persistent": false
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup":"popup.html"
    },
    "permissions":[
        "tabs",
        "<all_urls>",
        "storage"
    ]
}